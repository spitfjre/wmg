{
	"version": 1,
	"cli": {
		"defaultCollection": "@nrwl/angular"
	},
	"defaultProject": "client",
	"schematics": {
		"@nrwl/angular": {
			"application": {
				"linter": "eslint"
			},
			"library": {
				"linter": "eslint"
			},
			"storybook-configuration": {
				"linter": "eslint"
			}
		},
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"changeDetection": "OnPush",
			"displayBlock": true,
			"style": "scss"
		}
	},
	"projects": {
		"client": {
			"projectType": "application",
			"root": "apps/client",
			"sourceRoot": "apps/client/src",
			"prefix": "wmg",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/apps/client",
						"index": "apps/client/src/index.html",
						"main": "apps/client/src/main.ts",
						"polyfills": "apps/client/src/polyfills.ts",
						"tsConfig": "apps/client/tsconfig.app.json",
						"aot": true,
						"assets": ["apps/client/src/favicon.ico", "apps/client/src/assets"],
						"styles": [
							"./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
							"apps/client/src/styles.scss"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/client/src/environments/environment.ts",
									"with": "apps/client/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "client:build",
						"proxyConfig": "apps/client/proxy.conf.json"
					},
					"configurations": {
						"production": {
							"browserTarget": "client:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "client:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/client/src/**/*.ts", "apps/client/src/**/*.html"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/client"],
					"options": {
						"jestConfig": "apps/client/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"client-e2e": {
			"root": "apps/client-e2e",
			"sourceRoot": "apps/client-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/client-e2e/cypress.json",
						"tsConfig": "apps/client-e2e/tsconfig.e2e.json",
						"devServerTarget": "client:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "client:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/client-e2e/**/*.{js,ts}"]
					}
				}
			}
		},
		"expose-service": {
			"root": "apps/expose-service",
			"sourceRoot": "apps/expose-service/src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@nrwl/node:build",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/expose-service",
						"main": "apps/expose-service/src/main.ts",
						"tsConfig": "apps/expose-service/tsconfig.app.json",
						"assets": ["apps/expose-service/src/assets"]
					},
					"configurations": {
						"production": {
							"optimization": true,
							"extractLicenses": true,
							"inspect": false,
							"fileReplacements": [
								{
									"replace": "apps/expose-service/src/environments/environment.ts",
									"with": "apps/expose-service/src/environments/environment.prod.ts"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@nrwl/node:execute",
					"options": {
						"buildTarget": "expose-service:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/expose-service/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/expose-service"],
					"options": {
						"jestConfig": "apps/expose-service/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"source-service": {
			"root": "apps/source-service",
			"sourceRoot": "apps/source-service/src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@nrwl/node:build",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/source-service",
						"main": "apps/source-service/src/main.ts",
						"tsConfig": "apps/source-service/tsconfig.app.json",
						"assets": ["apps/source-service/src/assets"]
					},
					"configurations": {
						"production": {
							"optimization": true,
							"extractLicenses": true,
							"inspect": false,
							"fileReplacements": [
								{
									"replace": "apps/source-service/src/environments/environment.ts",
									"with": "apps/source-service/src/environments/environment.prod.ts"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@nrwl/node:execute",
					"options": {
						"buildTarget": "source-service:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/source-service/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/source-service"],
					"options": {
						"jestConfig": "apps/source-service/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"post": {
			"root": "libs/post",
			"sourceRoot": "libs/post/src",
			"projectType": "library",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/post/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/post"],
					"options": {
						"jestConfig": "libs/post/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		}
	}
}
